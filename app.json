{
  "name": "CMS API for LWC",
  "description": "CMS API for LWC",
  "keywords": ["python", "cloudinary", "Lightning Web Component"],
  "addons": [
    "cloudinary:starter"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "SALESFORCE_URL": {
      "required": true
    },
    "QUIP_ACCESS_TOKEN": {
      "required": true
    },
    "QUIP_CLIENT_ID": {
      "required": true
    },
    "QUIP_CLIENT_SECRET": {
      "required": true
    },
    "DOCS_FLAG": {
      "required": false
    }
  }
}